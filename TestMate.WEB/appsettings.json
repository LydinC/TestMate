{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //  "DeveloperPortalContext": "Server=(localdb)\\mssqllocaldb;Database=DeveloperPortal.Data;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "TestFrameworkDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "testframework_db"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/DeveloperPortalLog-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "MinimumLevel": "Information",
    "Properties": {
      "ApplicationName": "TestMate.WEB"
    }
  }
  //,
  //"HttpClient": {
  //  "Authorization": "Bearer Token"
  //}
}